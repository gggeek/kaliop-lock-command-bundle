{
  "name": "kaliop/console-bundle",
  "type": "symfony-bundle",
  "description": "Features for Symfony commands",
  "keywords": ["kaliop", "command", "console"],
  "license": "MIT",
  "authors": [
    {
      "name":     "Benoit Wannepain",
      "email":    "bwannepain@kaliop.com",
      "role":     "Lead Developer"
    }
  ],
  "require": {
    "symfony/http-kernel": "~2.7 || ~3",
    "symfony/dependency-injection": "~2.7 || ~3",
    "symfony/console": "~2.7 || ~3",
    "symfony/event-dispatcher": "~2.7 || ~3",
    "symfony/filesystem": "~2.7 || ~3",
    "symfony/config": "~2.7 || ~3"
  },
  "autoload": {
    "psr-4": { "Kaliop\\": "src" }
  },
  "minimum-stability": "stable",
  "extra": {
    "branch-alias": {
      "dev-master": "master"
    }
  }
}
